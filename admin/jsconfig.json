{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "baseUrl": "./",
        "moduleResolution": "node",
        "paths": {
            "@/*": [
                "src/*"
            ]
        }
    },
    "vueCompilerOptions": {
        "target": 2.7,
        "extensions": [
            ".vue"
        ]
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}